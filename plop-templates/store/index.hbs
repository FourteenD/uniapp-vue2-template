{{#if state}}
const state = {}
{{/if}}

{{#if mutations}}
const mutations = {
  setStateAttr(state, param) {
    if (param instanceof Array) {
      for (let item of param) {
        state[item.key] = item.val;
      }
    } else {
      state[param.key] = param.val;
    }
  },
}
{{/if}}

{{#if actions}}
const actions = {}
{{/if}}

export default {
  namespaced: true,
  {{options}}
}
